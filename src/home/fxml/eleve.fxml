<StackPane fx:id="elev" alignment="TOP_LEFT" prefHeight="675.0" prefWidth="1202.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="home.controllers.EleveController">
   <children>
      <VBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="668.0" prefWidth="1196.0" spacing="10.0">
