                        <Insets right="15.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Arabic Typesetting" size="24.0" />
                     </font>
                  </JFXButton>
                  <JFXButton buttonType="RAISED" onAction="#editElev" prefHeight="47.0" prefWidth="133.0" ripplerFill="#4264eb" styleClass="btn-view" stylesheets="@../css/eleve.css" text="تحديث">
