                        <Insets right="15.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Arabic Typesetting" size="24.0" />
                     </font>
                  </JFXButton>
                  <JFXButton buttonType="RAISED" onAction="#removeElev" prefHeight="39.0" prefWidth="130.0" ripplerFill="#4468eb" styleClass="btn-view" stylesheets="@../css/eleve.css" text="إزالة">
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Arabic Typesetting" size="24.0" />
                     </font>
                  </JFXButton>
               </children>
            </HBox>
            <JFXTreeTableView fx:id="treeTableView" fixedCellSize="0.0" prefHeight="496.0" prefWidth="1292.0" />
         </children>
         <StackPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
